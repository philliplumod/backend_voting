generator client {
  provider = "prisma-client-js"
}

model User {
  user_id         String   @id @default(uuid())
  username        String   @unique
  password        String
  email           String   @unique
  first_name      String
  middle_initial  String?  
  last_name       String
  suffix          String?  
  year_level      Int
  contact_number  String
  qr_code         String?  
  status          String
  created_at      DateTime @default(now())
}


datasource db {
  provider = "postgresql"
  url      = env("DATABASE_POSTGRES_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  user_id         String   @id @default(uuid())
  username        String   @unique     
  email           String   @unique     
  password        String                       
  first_name      String                     
  middle_initial  String?              
  last_name       String 
  suffix          String?
  contact_number  String                        // Contact number
  year_level      Int
  status          String                 
  created_at      DateTime @default(now())    
}



datasource db {
  provider = "postgresql"
  url      = env("DATABASE_POSTGRES_URL")
}
